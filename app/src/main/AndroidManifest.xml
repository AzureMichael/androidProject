<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ultimateorder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".waiter.WaiterActivity"
            android:label="@string/title_activity_waiter"
            android:parentActivityName=".sign_in.ui.SignInActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ultimateorder.sign_in.ui.SignInActivity" />
        </activity>
        <activity android:name=".sign_in.ui.SignInActivity" />
        <activity
            android:name=".ui.login.SignInActivity"
            android:label="@string/title_activity_sign_in"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.ultimateorder.MainActivity" />
        </activity>
        <activity
            android:name=".client.ClientActivity"
            android:label="@string/app_name" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>